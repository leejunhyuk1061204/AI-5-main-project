{
    "name": "ai-5-main-project-root",
    "version": "1.0.0",
    "description": "Root project to manage multi-service startup",
    "scripts": {
        "dev": "npm run dev:front",
        "server": "concurrently --kill-others-on-fail --prefix \"[{name}]\" --names \"BACK,AI\" --prefix-colors \"bgGreen.bold,bgYellow.bold\" \"npm run dev:back\" \"npm run dev:ai\"",
        "dev:front": "cd frontend && npx expo start -c --port 3000",
        "dev:back": "node scripts/runner.js backend",
        "dev:ai": "node scripts/runner.js ai"
    },
    "proxy": "http://localhost:3000",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}